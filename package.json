{
  "name": "mappersguild",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prod": "set NODE_ENV=production&& node ./dist/app.js",
    "build-src": "webpack --mode production --config webpack.prod.config.js",
    "build-app": "tsc --project tsconfig.app.json",
    "dev": "run-p -rl watch-app watch-src",
    "watch-app": "ts-node-dev --respawn --transpile-only --no-notify app.ts",
    "watch-src": "webpack serve --config webpack.dev.config.js",
    "lint": "eslint . --ext .ts,.js,.vue",
    "tsc": "tsc --noEmit true --project tsconfig.app.json"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "handlebars": "^4.7.6",
    "hbs": "^4.1.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.17",
    "morgan": "^1.10.0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.5.1",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/hbs": "^4.0.1",
    "@types/jquery": "^3.5.4",
    "@types/mongoose": "^5.10.1",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^7.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.0",
    "ts-loader": "^8.0.12",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "src/*"
    ]
  }
}
