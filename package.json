{
  "name": "mappersguild",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prod": "cross-env NODE_ENV=production node ./dist/api/app.js",
    "build-src": "vite build",
    "build-app": "cross-env NODE_ENV=production tsc --project api/tsconfig.json",
    "dev": "run-p -rl watch-app watch-src",
    "watch-app": "ts-node-dev --respawn --transpile-only --no-notify --project api/tsconfig.json api/app.ts",
    "watch-src": "vite",
    "serve": "vite preview",
    "lint": "eslint . --ext .ts,.js,.vue",
    "tsc": "tsc --noEmit true --project api/tsconfig.json"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "axios": "^0.21.4",
    "bootstrap": "^5.1.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "markdown-it": "^13.0.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.13.7",
    "morgan": "^1.10.0",
    "node-cron": "^2.0.3",
    "vue": "^3.2.4",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.4",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.16.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.38.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5",
    "vite": "^2.5.0",
    "vue-tsc": "^0.2.3"
  }
}
